[include]
  path = ~/.config/git/.gitconfig
[user]
  name = ya2s
[core]
  editor = nvim
  pager = delta
  excludesfile = /Users/yamatsum/.gitignore_global
[commit]
  template = /Users/yamatsum/.stCommitMsg
[ghq]
  root = ~/.ghq
[alias]
  unstage = reset HEAD
  get = !ghq get
[color "status"]
  added = green
  changed = yellow
  untracked = magenta
  deleted = red
[filter "lfs"]
  required = true
  clean = git-lfs clean -- %f
  smudge = git-lfs smudge -- %f
  process = git-lfs filter-process
[delta]
  syntax-theme = TwoDark
  line-numbers = true
